<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lazy.vm.mapper.TestPaperMapper">

    <resultMap type="TestPaper" id="TestPaperResult">
        <result property="id"    column="id"    />
        <result property="title"    column="title"    />
        <result property="fraction"    column="fraction"    />
        <result property="type"    column="type"    />
        <result property="category"    column="category"    />
        <result property="gmtCreate"    column="gmt_create"    />
        <result property="gmtModified"    column="gmt_modified"    />
        <result property="userId"    column="user_id"    />
        <result property="reserveField1"    column="reserve_field1"    />
        <result property="reserveField2"    column="reserve_field2"    />
        <result property="reserveField3"    column="reserve_field3"    />
        <result property="reserveField4"    column="reserve_field4"    />
        <result property="reserveField5"    column="reserve_field5"    />
    </resultMap>

    <sql id="selectTestPaperVo">
        select id, title, fraction, type, category, gmt_create, gmt_modified, user_id, reserve_field1, reserve_field2, reserve_field3, reserve_field4, reserve_field5 from edu_test_paper
    </sql>

    <select id="selectTestPaperList" parameterType="TestPaper" resultMap="TestPaperResult">
        <include refid="selectTestPaperVo"/>
        <where>
            <if test="title != null  and title != ''"> and title = #{title}</if>
            <if test="fraction != null "> and fraction = #{fraction}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="category != null "> and category = #{category}</if>
            <if test="gmtCreate != null "> and gmt_create = #{gmtCreate}</if>
            <if test="gmtModified != null "> and gmt_modified = #{gmtModified}</if>
            <if test="userId != null  and userId != ''"> and user_id = #{userId}</if>
            <if test="reserveField1 != null  and reserveField1 != ''"> and reserve_field1 = #{reserveField1}</if>
            <if test="reserveField2 != null  and reserveField2 != ''"> and reserve_field2 = #{reserveField2}</if>
            <if test="reserveField3 != null  and reserveField3 != ''"> and reserve_field3 = #{reserveField3}</if>
            <if test="reserveField4 != null  and reserveField4 != ''"> and reserve_field4 = #{reserveField4}</if>
            <if test="reserveField5 != null  and reserveField5 != ''"> and reserve_field5 = #{reserveField5}</if>
        </where>
    </select>

    <select id="selectTestPaperById" parameterType="String" resultMap="TestPaperResult">
        <include refid="selectTestPaperVo"/>
        where id = #{id}
    </select>

    <insert id="insertTestPaper" parameterType="TestPaper">
        insert into edu_test_paper
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="title != null and title != ''">title,</if>
            <if test="fraction != null">fraction,</if>
            <if test="type != null">type,</if>
            <if test="category != null">category,</if>
            <if test="gmtCreate != null">gmt_create,</if>
            <if test="gmtModified != null">gmt_modified,</if>
            <if test="userId != null">user_id,</if>
            <if test="reserveField1 != null">reserve_field1,</if>
            <if test="reserveField2 != null">reserve_field2,</if>
            <if test="reserveField3 != null">reserve_field3,</if>
            <if test="reserveField4 != null">reserve_field4,</if>
            <if test="reserveField5 != null">reserve_field5,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="title != null and title != ''">#{title},</if>
            <if test="fraction != null">#{fraction},</if>
            <if test="type != null">#{type},</if>
            <if test="category != null">#{category},</if>
            <if test="gmtCreate != null">#{gmtCreate},</if>
            <if test="gmtModified != null">#{gmtModified},</if>
            <if test="userId != null">#{userId},</if>
            <if test="reserveField1 != null">#{reserveField1},</if>
            <if test="reserveField2 != null">#{reserveField2},</if>
            <if test="reserveField3 != null">#{reserveField3},</if>
            <if test="reserveField4 != null">#{reserveField4},</if>
            <if test="reserveField5 != null">#{reserveField5},</if>
        </trim>
    </insert>

    <update id="updateTestPaper" parameterType="TestPaper">
        update edu_test_paper
        <trim prefix="SET" suffixOverrides=",">
            <if test="title != null and title != ''">title = #{title},</if>
            <if test="fraction != null">fraction = #{fraction},</if>
            <if test="type != null">type = #{type},</if>
            <if test="category != null">category = #{category},</if>
            <if test="gmtCreate != null">gmt_create = #{gmtCreate},</if>
            <if test="gmtModified != null">gmt_modified = #{gmtModified},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="reserveField1 != null">reserve_field1 = #{reserveField1},</if>
            <if test="reserveField2 != null">reserve_field2 = #{reserveField2},</if>
            <if test="reserveField3 != null">reserve_field3 = #{reserveField3},</if>
            <if test="reserveField4 != null">reserve_field4 = #{reserveField4},</if>
            <if test="reserveField5 != null">reserve_field5 = #{reserveField5},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTestPaperById" parameterType="String">
        delete from edu_test_paper where id = #{id}
    </delete>

    <delete id="deleteTestPaperByIds" parameterType="String">
        delete from edu_test_paper where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <insert id="addTestPaper"  parameterType="String">
        insert into  edu_answer_testPaper(answer_id,test_paper_id)value (#{answerId},#{testPaperId})
    </insert>
</mapper>