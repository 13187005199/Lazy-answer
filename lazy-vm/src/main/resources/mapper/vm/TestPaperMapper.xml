<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lazy.vm.mapper.TestPaperMapper">

    <resultMap type="TestPaper" id="TestPaperResult">
        <result property="id"    column="id"    />
        <result property="title"    column="title"    />
        <result property="totalScore"    column="total_score"    />
        <result property="type"    column="type"    />
        <result property="category"    column="category"    />
        <result property="gmtCreate"    column="gmt_create"    />
        <result property="gmtModified"    column="gmt_modified"    />
        <result property="userId"    column="user_id"    />
        <result property="quCount"    column="qu_count"    />
        <result property="objScore"    column="obj_score"    />
        <result property="subjScore"    column="subj_score"    />
        <result property="hasSaq"    column="has_saq"    />
        <result property="reserveField5"    column="reserve_field5"    />
    </resultMap>

    <sql id="selectTestPaperVo">
        select id, title, total_score, type, category, gmt_create, gmt_modified, user_id, qu_count, obj_score, subj_score, has_saq, reserve_field5 from edu_test_paper
    </sql>

    <select id="selectTestPaperList" parameterType="TestPaper" resultMap="TestPaperResult">
        <include refid="selectTestPaperVo"/>
        <where>
            <if test="title != null  and title != ''"> and title = #{title}</if>
            <if test="totalScore != null "> and total_score = #{totalScore}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="category != null "> and category = #{category}</if>
            <if test="gmtCreate != null "> and gmt_create = #{gmtCreate}</if>
            <if test="gmtModified != null "> and gmt_modified = #{gmtModified}</if>
            <if test="userId != null  and userId != ''"> and user_id = #{userId}</if>
            <if test="quCount != null  and reserveField1 != ''"> and qu_count = #{quCount}</if>
            <if test="objScore != null  and objScore != ''"> and obj_score = #{objScore}</if>
            <if test="subjScore != null  and subjScore != ''"> and subj_score = #{subjScore}</if>
            <if test="hasSaq != null  and hasSaq != ''"> and has_saq = #{hasSaq}</if>
            <if test="reserveField5 != null  and reserveField5 != ''"> and reserve_field5 = #{reserveField5}</if>
        </where>
    </select>

    <select id="selectTestPaperById" parameterType="String" resultMap="TestPaperResult">
        <include refid="selectTestPaperVo"/>
        where id = #{id}
    </select>

    <insert id="insertTestPaper" parameterType="TestPaper">
        insert into edu_test_paper
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="title != null and title != ''">title,</if>
            <if test="totalScore != null">total_score,</if>
            <if test="type != null">type,</if>
            <if test="category != null">category,</if>
            <if test="gmtCreate != null">gmt_create,</if>
            <if test="gmtModified != null">gmt_modified,</if>
            <if test="userId != null">user_id,</if>
            <if test="quCount != null">qu_count,</if>
            <if test="objScore != null">obj_score,</if>
            <if test="subjScore != null">subj_score,</if>
            <if test="hasSaq != null">has_saq,</if>
            <if test="reserveField5 != null">reserve_field5,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="title != null and title != ''">#{title},</if>
            <if test="totalScore != null">#{totalScore},</if>
            <if test="type != null">#{type},</if>
            <if test="category != null">#{category},</if>
            <if test="gmtCreate != null">#{gmtCreate},</if>
            <if test="gmtModified != null">#{gmtModified},</if>
            <if test="userId != null">#{userId},</if>
            <if test="quCount != null">#{quCount},</if>
            <if test="objScore != null">#{objScore},</if>
            <if test="subjScore != null">#{subjScore},</if>
            <if test="hasSaq != null">#{hasSaq},</if>
            <if test="reserveField5 != null">#{reserveField5},</if>
        </trim>
    </insert>

    <update id="updateTestPaper" parameterType="TestPaper">
        update edu_test_paper
        <trim prefix="SET" suffixOverrides=",">
            <if test="title != null and title != ''">title = #{title},</if>
            <if test="totalScore != null">total_score = #{totalScore},</if>
            <if test="type != null">type = #{type},</if>
            <if test="category != null">category = #{category},</if>
            <if test="gmtCreate != null">gmt_create = #{gmtCreate},</if>
            <if test="gmtModified != null">gmt_modified = #{gmtModified},</if>
            <if test="userId != null">user_id = #{userId},</if>
            <if test="quCount != null">qu_count = #{quCount},</if>
            <if test="objScore != null">obj_score = #{objScore},</if>
            <if test="subjScore != null">subj_score = #{subjScore},</if>
            <if test="hasSaq != null">has_saq = #{hasSaq},</if>
            <if test="reserveField5 != null">reserve_field5 = #{reserveField5},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTestPaperById" parameterType="String">
        delete from edu_test_paper where id = #{id}
    </delete>

    <delete id="deleteTestPaperByIds" parameterType="String">
        delete from edu_test_paper where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

    <insert id="addTestPaper"  parameterType="String">
        insert into  edu_answer_testPaper(answer_id,test_paper_id)value (#{answerId},#{testPaperId})
    </insert>
</mapper>
